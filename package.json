{
  "name": "la_glekitufa",
  "version": "0.0.1",
  "description": "A combine of Lojban tools",
  "dependencies": {
    "byline": "^5.0.0",
    "irc": "~0.5.0",
    "libxmljs": "~0.18.0",
    "line-by-line": "~0.1.3",
    "lojban": "*",
    "nodemw": "~0.8.0",
    "ospath": "~1.2.2",
    "path-extra": "~0.2.1",
    "pegjs": "~0.10.0",
    "request": "~2.55.0",
    "seek-bzip": "~1.0.4",
    "socket.io": "~1.3.5",
    "sqlite3": "^3.1.8",
    "supervisor": "~0.6.0",
    "tarball-extract": "~0.0.3",
    "uglify-js": "~2.6.1",
    "v8-profiler": "^5.6.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lagleki/glekitufa"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^3.8.1",
    "flow-bin": "^0.41.0"
  },
  "main": "livla/livla.js",
  "scripts": {
    "livla": "node node_modules/.bin/supervisor -w .,./livla -i ./dumps,./sutysisku,./mahantufa -- --expose-gc livla/livla.js",
    "sutysisku": "cd sutysisku ; node .scripts/morna.js ; git add . ; git commit -m 'commit' ; git push ; cd ..",
    "snovlaste": "cd sutysisku ; node .scripts/vlaste.js ; node .scripts/morna.js ; git checkout --orphan newBranch ; git add -A ; git commit -m 'commit' ; git branch -D master ; git branch -m master ; git push -f origin master ; git gc --aggressive --prune=all ; git push --set-upstream origin master ; cd ..",
    "push": "git add . ; git commit -a ;  git push",
    "compile": "./node_modules/.bin/babel src/ -d lib/",
    "build": "babel src/ -d lib/",
    "prepublish": "npm run build",
    "flow": "flow"
  }
}
